<table cellpadding="0" cellspacing="0" border="0" 
      class="table table-striped table-bordered" id="tableview">
  <thead>
    <tr> 
      <th>Tanggal</th>
      <th>Jumlah</th>
      <th>Keterangan</th>
      <th>Tindakan</th> 
    </tr>
  </thead>
  <tbody>
    <% transaction_activities.each do |transaction_activity| %>
			<%= render :partial => "savings_entries/savings_accounts/entry", :locals => {
				:transaction_activity => transaction_activity
			}%>
    <% end %>

  </tbody>
</table>