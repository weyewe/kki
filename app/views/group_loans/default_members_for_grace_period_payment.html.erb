<div class="span9 content">
  <%= create_guide("#{t 'process.grace_period_payment'}", 
        "Office: <b>#{@office.name}</b>" + "<br />" + 
        "Group Loan: <b>#{@group_loan.name}</b> " + "<br />" + 
        "#{t 'variable.amount'}: <b>#{ print_money( @group_loan.unpaid_grace_period_amount ) }</b>").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th><%= t 'report_metric.member' %></th> 
            <th><%= t 'report_metric.number_of_transactions'%></th>
            <th><%= t 'guide.total_unpaid_amount'%></th>
            <th><%= t 'trigger.action' %></th>
						<th>Pending Cash Approval Amount<%#= t 'trigger.action' %></th>
          </tr>
        </thead>
        <tbody>
          <% @group_loan_memberships.each do |glm| %>
            <%= render :partial => "group_loans/grace_period_payment_entry" , 
                :locals => {:group_loan_membership => glm} %>
          <% end %>
       
          

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
  
  
  
  <div class=" hidden" id="approve_propose_form_wrapper">
    <%= form_for GroupLoan.new, :url => execute_loan_disbursement_finalization_url   do |f| %>
      <%= hidden_field_tag 'entity_id' , ''  %>
      <%= hidden_field_tag 'action_role', '' %>
      <%= hidden_field_tag 'action_value', '' %>
    <% end %>
  </div>
  
  
  <div class="modal" id="myModal" style="display:none;">
     <div class="modal-header">
       <a class="close" data-dismiss="modal">×</a>
       <h3>Modal header</h3>
     </div>
     <div class="modal-body">
       <p>One fine body…</p>
     </div>
     <div class="modal-footer">
       <a href="#" class="btn modal-closer"><%= t 'trigger.close' %></a>
     </div>
   </div>
  
  
  

</div><!--/span-->















