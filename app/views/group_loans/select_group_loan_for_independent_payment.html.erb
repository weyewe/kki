
<div class="span9 content">
  <%= create_guide("Independent Payment", 
        "Select Group Loan").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Nama Group</th>
            <th>Lokasi RW</th>
            <th>Total Member</th>
            <th>Total Backlog</th>
            <th>Cleared Backlog</th>
          </tr>
        </thead>
        <tbody>
          <% @running_group_loans.each do |group_loan| %>
            <%= render :partial => "group_loans/group_loan_with_backlog" , 
                :locals => {:group_loan => group_loan}%>
          <% end %>
        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
  
 
  

</div><!--/span-->















