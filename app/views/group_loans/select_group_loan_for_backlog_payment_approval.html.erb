<div class="span9 content">
  <%= create_guide("#{t 'process.select_group_loan'}", 
        "Group loan for office: <b>#{@office.name}</b>" + "<br />" + 
        "Active Group Loans: <b>#{@active_group_loans.count}</b> ").html_safe %>
  
  <%#= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th><%= t 'report_metric.group_name' %></th>
            <th>Lokasi RW</th>
            <th><%= t 'report_metric.member' %></th>
            <th><%= t 'report_metric.total_backlog' %></th>
            <th>Total Unpaid Backlog</th>
            <th><%= t 'report_metric.pending_approval'%></th>
          </tr>
        </thead>
        <tbody>
          <% @active_group_loans.each do |group_loan| %>
            <%#= render :partial => "group_loans/group_loan_setup" , 
                :locals => {:group_loan => group_loan}%>
            <%= render :partial => "group_loans/group_loan_backlog_payment_approval" , 
                :locals => {:group_loan => group_loan}%>
          <% end %>
       
          

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
  
  
  
  <!-- <div class=" hidden" id="approve_propose_form_wrapper">
     <%#= form_for GroupLoan.new, :url => execute_backlog_payment_approval_by_cashier_url   do |f| %>
       <%#= hidden_field_tag 'entity_id' , ''  %>
       <%#= hidden_field_tag 'action_role', '' %>
       <%#= hidden_field_tag 'action_value', '' %>
     <%# end %>
   </div> -->
  
  
  <div class="modal" id="myModal" style="display:none;">
     <div class="modal-header">
       <a class="close" data-dismiss="modal">×</a>
       <h3>Modal header</h3>
     </div>
     <div class="modal-body">
       <p>One fine body…</p>
     </div>
     <div class="modal-footer">
       <a href="#" class="btn modal-closer"><%= t 'trigger.close' %></a>
     </div>
   </div>
  
  
  

</div><!--/span-->















