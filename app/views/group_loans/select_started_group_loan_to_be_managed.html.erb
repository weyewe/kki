
<div class="span9 content">
  <%= create_guide("#{t 'guide.group_loan_progress'}", 
        "#{t 'guide.office'}: <b>#{@office.name}</b>" + "<br />" + 
        "#{t 'guide.started_group_loan'}: <b>#{@started_group_loans.count}</b> ").html_safe %>
  
  <%#= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th><%= t 'report_metric.group_name' %></th>
            <th>Lokasi RW</th>
            <th>Total Member</th>
            <th><%= t 'guide.financial_education' %></th>
            <th><%= t 'process.loan_disbursement' %></th>
            <th><%= t 'report_metric.grace_period' %></th>
            <th><%= t 'report_metric.default_group_resolution_proposed' %> </th>
            <th><%= t 'report_metric.default_group_resolution_executed' %></th>
            <th><%= t 'report_metric.close_group_loan' %></th>
       
         
            
          </tr>
        </thead>
        <tbody>
          
          <% @started_group_loans.each do |group_loan| %>
            <%= render :partial => "group_loans/monitor_started_group_loan" , 
                :locals => {:group_loan => group_loan} %>
          <% end %>
          
        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
</div><!--/span-->



<div class=" hidden" id="approve_propose_form_wrapper">
  <%= form_for GroupLoan.new, :url =>close_group_loan_url   do |f| %>
    <%= hidden_field_tag 'entity_id' , ''  %>
    <%= hidden_field_tag 'action_role', '' %>
    <%= hidden_field_tag 'action_value', '' %>
  <% end %>
</div>















