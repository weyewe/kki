
<div class="span9 content">
  <%= create_guide("Group Loan Progress", 
        "Monitor group loan for office: <b>#{@office.name}</b>" + "<br />" + 
        "Started Group Loan: <b>#{@started_group_loans.count}</b> ").html_safe %>
  
  <%#= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Nama Group</th>
            <th>Lokasi RW</th>
            <th>Total Member</th>
            <th>Financial Education</th>
            <th>Loan Disbursement</th>
            <th>Grace Period</th>
            <th>Default Resolution Proposed</th>
            <th>Default Resolution Executed</th>
            <th>Close Group Loan</th>
            <!-- <th>Total Collected</th> -->
       
         
            
          </tr>
        </thead>
        <tbody>
          
          <% @started_group_loans.each do |group_loan| %>
            <%= render :partial => "group_loans/monitor_started_group_loan" , 
                :locals => {:group_loan => group_loan} %>
          <% end %>
          
        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
</div><!--/span-->















