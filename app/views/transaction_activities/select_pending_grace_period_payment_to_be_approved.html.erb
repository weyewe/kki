
<div class="span9 content">
  <%= create_guide("#{t 'process.approve_grace_period_payment'}", 
        "#{t 'guide.office'}: <b>#{@office.name}</b>" + "<br />" + 
        "#{t 'guide.group_loan'}: <b>#{@group_loan.name}</b>").html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th><%= t 'report_metric.member' %></th>
            <!-- <th>Backlogs Paid</th> -->
            <th><%= t 'report_metric.transaction_code'%></th>
            <th><%= t 'report_metric.transaction_amount'%></th>
            <th><%= t 'trigger.action' %></th>
          </tr>
        </thead>
        <tbody>
          <% @grace_period_transactions.each do |transaction_activity| %>
            <%= render :partial => "transaction_activities/grace_period_payment_entry" , 
                :locals => {:transaction_activity => transaction_activity}%>
          <% end %>
       
          

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
  
  <div class=" hidden" id="approve_propose_form_wrapper">
    <%= form_for GroupLoan.new, :url =>execute_backlog_payment_transaction_approval_by_cashier_url   do |f| %>
      <%= hidden_field_tag 'entity_id' , ''  %>
      <%= hidden_field_tag 'action_role', '' %>
      <%= hidden_field_tag 'action_value', '' %>
    <% end %>
  </div>
  
  
  <div class="modal" id="myModal" style="display:none;">
     <div class="modal-header">
       <a class="close" data-dismiss="modal">×</a>
       <h3>Modal header</h3>
     </div>
     <div class="modal-body">
       <p>One fine body…</p>
     </div>
     <div class="modal-footer">
       <a href="#" class="btn modal-closer"><%= t 'trigger.close' %></a>
       <!-- <a href="#" class="btn btn-primary">Save changes</a> -->
     </div>
   </div>
  
  

</div><!--/span-->















