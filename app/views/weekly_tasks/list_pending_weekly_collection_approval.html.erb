
<div class="span9 content">
  <%= create_guide("Group Loan Collection Approval", 
        "Group loan for office: <b>#{@office.name}</b>" + "<br />" + 
        "Total Weekly Tasks Pending Approval: <b>#{@pending_weekly_tasks_cashier_approval.count}</b> ").html_safe %>
  
  <%#= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Nama Group</th>
            <th>Lokasi RW</th>
            <th>Week </th>
            <th>Finalizer ID</th>
            <th>Finalization Time</th>
            <th>Expected Cash</th>
            <th style="text-align:center;" colspan='2'>Action</th>
          </tr>
        </thead>
        <tbody>
          <% @pending_weekly_tasks_cashier_approval.each do |weekly_task| %>
            <%= render :partial => "weekly_tasks/cashier_collection_approval" , 
                :locals => {:weekly_task => weekly_task}%>
   
          <% end %>
       
          

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
  
  
  
  <div class=" hidden" id="approve_propose_form_wrapper">
    <%= form_for WeeklyTask.new, :url => execute_weekly_collection_approval_url   do |f| %>
      <%= hidden_field_tag 'entity_id' , ''  %>
      <%= hidden_field_tag 'action_role', '' %>
      <%= hidden_field_tag 'action_value', '' %>
    <% end %>
  </div>
  
  
  <div class="modal" id="myModal" style="display:none;">
     <div class="modal-header">
       <a class="close" data-dismiss="modal">×</a>
       <h3>Modal header</h3>
     </div>
     <div class="modal-body">
       <p>One fine body…</p>
     </div>
     <div class="modal-footer">
       <a href="#" class="btn modal-closer">Close</a>
       <!-- <a href="#" class="btn btn-primary">Save changes</a> -->
     </div>
   </div>
   
   <!-- <div class=" hidden" id="approve_propose_form_wrapper">
       <%#= form_for GroupLoan.new, :url => approve_setup_fee_collection_url   do |f| %>
         <%#= hidden_field_tag 'entity_id' , ''  %>
         <%#= hidden_field_tag 'action_role', '' %>
         <%#= hidden_field_tag 'action_value', '' %>
       <%# end %>
     </div> -->



    
    
  
  
  

</div><!--/span-->















