<div class="span9 content">
  <%= create_guide("Default Loan Resolution", 
        "Group loan for office: <b>#{@office.name}</b>" + "<br />" + 
        "Total Default Payment: <b>#{@default_payments.count}</b> " + "<br />" + 
        "Paid Default Payment: <b>#{@group_loan.members_paid_default_payment.count}</b> "  ).html_safe %>
  
  <%= create_breadcrumb(@breadcrumbs).html_safe %>
  
  
   
  <div class="row-fluid">
    <div id="data_list">
      <table cellpadding="0" cellspacing="0" border="0" 
            class="table table-striped table-bordered" id="example">
        <thead>
          <tr>
            <th>Nama Member</th>
            <th>SubGroup Share</th>
            <th>Group Share</th>
            <th>Total Rounded</th>
            <th>Savings Deduction</th>
            <th>Office Paid</th>
            
            
            <th style="text-align:center;">Total Savings Post Default Deduction</th>
            
          </tr>
        </thead>
        <tbody>
          <% @default_payments.each do |default_payment| %>
            <%#= render :partial => "group_loans/group_loan_setup" , 
                :locals => {:group_loan => group_loan}%>
            <%= render :partial => "default_payments/default_payment_entry" , 
                :locals => {:default_payment => default_payment}%>
          <% end %>
       
          

        </tbody>
      </table>
    </div><!-- end of #data_list -->
  </div>
  
  
  
  <div class=" hidden" id="approve_propose_form_wrapper">
    <%= form_for GroupLoan.new, :url => execute_backlog_payment_approval_by_cashier_url   do |f| %>
    <%= hidden_field_tag 'entity_id' , ''  %>
    <%= hidden_field_tag 'action_role', '' %>
    <%= hidden_field_tag 'action_value', '' %>
    <% end %>
  </div>


  <div class="modal" id="myModal" style="display:none;">
     <div class="modal-header">
       <a class="close" data-dismiss="modal">×</a>
       <h3>Modal header</h3>
     </div>
     <div class="modal-body">
       <p>One fine body…</p>
     </div>
     <div class="modal-footer">
       <a href="#" class="btn modal-closer">Close</a>
       <!-- <a href="#" class="btn btn-primary">Save changes</a> -->
     </div>
   </div>
  
  
  

</div><!--/span-->















